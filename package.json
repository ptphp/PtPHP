{
  "name": "ptreact",
  "version": "0.0.1",
  "description": "react",
  "scripts": {
    "start": "npm run dev:php | npm run dev:react",
    "dev:php": "php -S 127.0.0.1:3080 -t ./webroot ./etc/php/router.php",
    "dev:react": "NODE_ENV=development webpack-dev-server --hot --inline --progress --colors --port 3079 --host 127.0.0.1",
    "build:pre": "rimraf ./webroot/mission/static/build/css/ ./webroot/mission/static/build/js/ ./webroot/mission/static/build/img/ ./webroot/mission/static/build/webpack-assets.json",
    "build:after": "touch ./webroot/mission/static/assets/webpack-assets.json && cp ./webroot/mission/static/assets/webpack-assets.json ./logs/webpack-assets.$(date \"+%Y_%m_%d_%H_%M_%S\").json && cp -R ./webroot/mission/static/build/* ./webroot/mission/static/assets/",
    "build": "npm run build:pre && NODE_ENV=production webpack && npm run build:after",
    "clean": "rimraf ./lib ./coverage",
    "fix_antd": "cp -R ./node_modules/antd/lib/breadcrumb/index.js ./node_modules/antd/lib/breadcrumb/index_back.js && cp ./docs/res/breadcrumb/index.js ./node_modules/antd/lib/breadcrumb/index.js",
    "build:weui": "npm run clean & babel ./src --out-dir ./lib",
    "lint": "eslint 'src/**/*.@(js|jsx)'",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run lint & istanbul cover _mocha -- --compilers js:babel-core/register --recursive"
  },
  "repository": {},
  "keywords": [
    "mobile",
    "ui",
    "framework",
    "weui",
    "react"
  ],
  "author": "joseph",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "classnames": "^2.2.0",
    "react": "^0.14.2"
  },
  "devDependencies": {
    "antd": "^0.12.13",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.5",
    "babel": "^5.8.23",
    "babel-core": "^5.8.38",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "css-loader": "^0.23.0",
    "enzyme": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "html-webpack-plugin": "^2.14.0",
    "istanbul": "^0.4.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.4",
    "open-browser-webpack-plugin": "0.0.1",
    "postcss-loader": "^0.8.2",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.2",
    "react-icons": "^2.1.0",
    "react-router": "^2.0.1",
    "react-sentry": "^1.0.2",
    "react-tappable": "^0.8.1",
    "react-weui": "^0.2.0",
    "reqwest": "^2.0.5",
    "rimraf": "^2.4.3",
    "simditor": "^2.3.6",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "swiper": "^3.3.1",
    "uglify-loader": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "weui": "^0.4.0"
  }
}
